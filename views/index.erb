<div class="container">
  <table class="table">
    <h2>Results</h2>      
      <thead>
        <tr>
          <th>id</th>
          <th>subject</th>
        </tr>
      </thead>
      <tbody>
      <%@tickets.each do |ticket|%>
      <tr>
        <td><%= ticket["id"]%></td>
        <td><a href='/ticket/<%= ticket["id"]%>'><%= ticket["subject"]%></a></td>
      </tr>
      <%end%>
      </tbody>
  </table>
</div>